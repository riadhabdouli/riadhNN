<form method="post" action="{{ path('secteur_delete', {'idSecteur': secteur.idSecteur}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ secteur.idSecteur) }}">
    <button class="btn">Delete</button>
</form>
