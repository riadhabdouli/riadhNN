            <tr>
                <th>NomEmploi</th>
                <td>{{ emploi.nomEmploi }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('emploi_index') }}">back to list</a>

    <a href="{{ path('emploi_edit', {'idEmploi': emploi.idEmploi}) }}">edit</a>

    {{ include('emploi/_delete_form.html.twig') }}
