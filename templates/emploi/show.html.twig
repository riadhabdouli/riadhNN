            <tr>
                <th>Responsabilite</th>
                <td>{{ emploi.responsabilite }}</td>
            </tr>
            <tr>
                <th>Agemin</th>
                <td>{{ emploi.agemin }}</td>
            </tr>
            <tr>
                <th>Agemax</th>
                <td>{{ emploi.agemax }}</td>
