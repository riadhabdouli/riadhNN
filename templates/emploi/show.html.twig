{% extends 'base.html.twig' %}

{% block title %}Emploi{% endblock %}

{% block body %}
    <h1>Emploi</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdEmploi</th>
                <td>{{ emploi.idEmploi }}</td>
            </tr>
            <tr>
                <th>Experience</th>
                <td>{{ emploi.experience }}</td>
            </tr>
            <tr>
                <th>NiveauEtude</th>
                <td>{{ emploi.niveauEtude }}</td>
            </tr>
            <tr>
                <th>Disponibilite</th>
                <td>{{ emploi.disponibilite }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ emploi.sexe }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ emploi.dateDebut ? emploi.dateDebut|date('Y-m-d') : '' }}</td>
