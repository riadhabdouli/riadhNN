<form method="post" action="{{ path('emploi_delete', {'idEmploi': emploi.idEmploi}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ emploi.idEmploi) }}">
    <button class="btn">Delete</button>
</form>
